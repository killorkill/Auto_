        function mapGoogle(xmap, ymap, zmap, xText, xdiv) {
            var loc, map, marker, infobox;

            loc = new google.maps.LatLng(xmap, ymap, zmap);

            map = new google.maps.Map(document.getElementById(xdiv), {
                zoom: 14,
                center: loc,
                mapTypeId: google.maps.MapTypeId.ROADMAP
            });

            marker = new google.maps.Marker({
                map: map,
                position: loc,
                visible: true
            });

            infobox = new InfoBox({
                content: xText,
                disableAutoPan: true,
                pixelOffset: new google.maps.Size(-138, -103),
                zIndex: null,
                closeBoxURL: "/App_Themes/2016-Styles/images/icon/close.png",
                infoBoxClearance: new google.maps.Size(1, 1)
            });

            google.maps.event.addListener(marker, 'click', function () {
                infobox.open(map, marker);
                map.panTo(loc);
                marker.setVisible(false);
            });

            google.maps.event.addListener(infobox, 'closeclick', function () {
                marker.setVisible(true);
                // then, remove the infowindows name from the array
            });

            infobox.open(map, marker);
            map.panTo(loc);
            marker.setVisible(false);
